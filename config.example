[mbmain]
show_mesg = False
bind_enable = alt-e
bind_disable = alt-d

[define]
dictionary_file = dictd_www.dict.org_gcide
rofi_lines = 25

[thesaurus]
thesaurus_file = Moby Thesaurus II
rofi_lines = 25

[screenshots]
screenshot_directory = ~/Pictures/screenshots/
bind_upload = alt-u
bind_preview = alt-p
bind_rename = alt-r
bind_sortname = alt-1
bind_sortdate = alt-2
bind_sortsize = alt-3

[sshot]
screenshot_directory = ~/Pictures/screenshots/
bind_upload = alt+u
bind_delay = alt+d
bind_interactive = alt+i
entry_order = selection,all,displays
upload = False
maim_bordersize = 3
maim_color = 0.7,0.3,0.7,1.0
